{
  "files.exclude": {
    "**/.git": true,
    "**/*.js.map": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*?.js": {
      "when": "$(basename).tsx"
    }
  },
  "files.eol": "\n",
  "editor.tabSize": 2,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false
}
